{
  "network_latency": ".02+.002-0.001",
  "microservices": [
    {
      "name": "gateway",
      "instances": 1,
      "capacity": 10,
      "loadbalancer_strategy": "even",
      "operations": [
        {
          "name": "gateway.PING",
          "demand": 1,
          "dependencies": []
        },
        {
          "name": "INTERNAL_DEP",
          "demand": 1,
          "dependencies": [
            {
              "service": "example-service",
              "operation": "call_example-service2",
              "custom_delay": ".5+-.1"
            }
          ]
        },
        {
          "name": "EXTERNAL_DEP",
          "demand": 1,
          "dependencies": [
            {
              "operation": "example-service.call_external_service",
              "probability": "0.75"
            }
          ]
        },
        {
          "name": "gateway.DB_READ",
          "demand": 1,
          "dependencies": [
            {
              "service": "example-service",
              "operation": "example-service.DB_READ"
            }
          ]
        },
        {
          "name": "gateway.DB_WRITE",
          "demand": 1,
          "dependencies": [
            {
              "service": "example-service",
              "operation": "example-service.DB_WRITE"
            }
          ]
        },
        {
          "name": "UNAFFECTED_SERVICE",
          "demand": 1,
          "dependencies": [
            {
              "service": "example-service2",
              "operation": "example-service2.get"
            }
          ]
        }
      ],
      "patterns": [
        {
          "type": "circuitbreaker",
          "config": {
            "error_threshold_percentage": 0.7
          }
        },
        {
          "type": "retry",
          "config": {
            "maxTries": 5
          },
          "strategy": {
            "type": "jittering",
            "config": {
              "baseBackoff": 0.02,
              "maxBackoff": 1,
              "base": 2
            }
          }
        }
      ],
      "s_patterns": [
        {
          "type": "autoscaling",
          "config": {
            "period": 0.5
          },
          "strategy": {
            "type": "reactive",
            "config": {
              "lower_bound": 0.1,
              "upper_bound": 0.9,
              "hold_time": 60
            }
          }
        }
      ]
    },
    {
      "name": "example-service",
      "instances": 1,
      "patterns": [],
      "capacity": 15000,
      "operations": [
        {
          "name": "example-service.DB_WRITE",
          "demand": 100,
          "dependencies": []
        },
        {
          "name": "example-service.DB_READ",
          "demand": 100,
          "dependencies": []
        },
        {
          "name": "call_example-service2",
          "demand": 100,
          "dependencies": [
            {
              "service": "example-service2",
              "operation": "example-service2.get"
            }
          ]
        },
        {
          "name": "call_external_service",
          "demand": 100,
          "dependencies": []
        }
      ]
    },
    {
      "name": "example-service2",
      "instances": 1,
      "patterns": [],
      "capacity": 32000,
      "operations": [
        {
          "name": "example-service2.get",
          "demand": 1,
          "dependencies": []
        }
      ]
    }
  ]
}