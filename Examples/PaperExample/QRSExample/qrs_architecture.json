{
  "network_latency": ".02+.002-0.001",
  "microservices": [
    {
      "name": "gateway",
      "instances": 1,
      "capacity": 10000,
      "loadbalancer_strategy": "even",
      "operations": [
        {
          "name": "API_Endpoint",
          "demand": 1,
          "dependencies": [
            {
              "service": "Service1",
              "operation": "dependentCalculation"
            }
          ]
        }
      ],
      "patterns": [
        {
          "type": "retry",
          "strategy": {
            "type": "linear"
          }
        }
      ]
    },
    {
      "name": "Service1",
      "instances": 2,
      "patterns": [],
      "capacity": 75,
      "operations": [
        {
          "name": "dependentCalculation",
          "demand": 10,
          "dependencies": [
            {
              "operation": "Service2.doWork",
              "probability": 0.75
            }
          ]
        }
      ],
      "s_patterns": [
        {
          "type": "autoscaling",
          "config": {
            "period": 0.5
          },
          "strategy": {
            "type": "reactive",
            "config": {
              "lower_bound": 0.2,
              "upper_bound": 0.8
            }
          }
        }
      ]
    },
    {
      "name": "Service2",
      "instances": 1,
      "capacity": 32000,
      "operations": [
        {
          "name": "doWork",
          "demand": 1
        }
      ]
    }
  ]
}
